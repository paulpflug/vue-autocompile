{
  "name": "vue-autocompile",
  "main": "./lib/vue-autocompile",
  "version": "0.0.5",
  "description": "Auto compiles vue in atom",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulpflug/vue-autocompile"
  },
  "activationCommands": {
    "atom-text-editor": [
      "core:save"
    ]
  },
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "coffee-script": "^1.10.0",
    "jade": "^1.11.0",
    "less": "^2.7.1",
    "node-sass": "^3.7.0",
    "pug": "^2.0.0-alpha8",
    "stylus": "^0.54.5",
    "vue-compiler": "~0.3.0"
  },
  "consumedServices": {
    "autoreload": {
      "versions": {
        "^0.0.1": "consumeAutoreload"
      }
    },
    "debug": {
      "versions": {
        "^0.0.1": "consumeDebug"
      }
    }
  }
}
